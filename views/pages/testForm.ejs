<% include ../partials/head %>

	<main class="slds-card">
		<div>
			This is a form for testing.  The real
		</div>
		<form class="slds-form slfd-form_stacked slds-m-top--large" action="/trial?template=https://github.com/mshanemc/platformTrial">
			<div class="slds-form-element">
				<label class="slds-form-element__label" for="input-unique-id">Email</label>
				<div class="slds-form-element__control">
					<input type="email" id="UserEmail" class="slds-input" name="UserEmail" />
				</div>
			</div>
			<div class="slds-form-element">
				<label class="slds-form-element__label" for="input-unique-id">First name</label>
				<div class="slds-form-element__control">
					<input type="text" id="UserFirstName" class="slds-input" name="UserFirstName" />
				</div>
			</div>
			<div class="slds-form-element">
				<label class="slds-form-element__label" for="input-unique-id">Last name</label>
				<div class="slds-form-element__control">
					<input type="text" id="UserLastName" class="slds-input" name="UserLastName" />
				</div>
			</div>
			<input class="slds-button slds-button_brand slds-m-top_small" value="Submit">Go</button>
		</form>
	</main>

	<% include ../partials/footer %>